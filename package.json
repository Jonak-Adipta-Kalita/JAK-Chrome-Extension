{
    "name": "jak-chrome-extension",
    "version": "0.0.1",
    "description": "A Chrome Extension made by JAK",
    "main": "src/index.js",
    "scripts": {
        "test": "echo No Tests provided till now.",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prettier": "prettier **/*.{js,html,css,md,json}",
        "prettier:fix": "prettier --write **/*.{js,html,css,md,json}",
        "build:popup": "webpack --mode production --entry ./src/popup/index.js --output-filename script.js -o ./dist/popup",
        "watch": "webpack --watch",
        "format": "eslint --fix . && prettier --write **/*.{js,html,css,md,json}",
        "check:version": "ncu",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Jonak-Adipta-Kalita/JAK-Chrome-Extension.git"
    },
    "keywords": [
        "chrome",
        "extension"
    ],
    "author": "Jonak Adipta Kalita",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Jonak-Adipta-Kalita/JAK-Chrome-Extension/issues"
    },
    "homepage": "https://github.com/Jonak-Adipta-Kalita/JAK-Chrome-Extension#readme",
    "devDependencies": {
        "eslint": "^7.32.0",
        "husky": "^7.0.4",
        "npm-check-updates": "^12.0.2",
        "prettier": "^2.4.1",
        "webpack": "^5.64.4",
        "webpack-cli": "^4.9.1"
    },
    "dependencies": {}
}
