{
    "name": "jak-chrome-extension",
    "version": "0.0.1",
    "description": "A Chrome Extension made by JAK",
    "main": "dist/background.js",
    "scripts": {
        "test": "echo No Tests provided till now.",
        "build": "tsc && webpack --config webpack.config.js",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prettier": "prettier **/*.{js,html,css,md,json}",
        "prettier:fix": "prettier --write **/*.{js,html,css,md,json}",
        "format": "eslint --fix . && prettier --write **/*.{js,html,css,md,json}",
        "check:version": "ncu",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Jonak-Adipta-Kalita/JAK-Chrome-Extension.git"
    },
    "keywords": [
