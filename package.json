    "bugs": {
        "url": "https://github.com/Jonak-Adipta-Kalita/JAK-Chrome-Extension/issues"
    },
    "homepage": "https://github.com/Jonak-Adipta-Kalita/JAK-Chrome-Extension#readme",
    "devDependencies": {
        "@types/chrome": "^0.0.186",
        "copy-webpack-plugin": "^10.2.4",
        "eslint": "^7.32.0",
        "husky": "^7.0.4",
        "npm-check-updates": "^16.3.3",
        "prettier": "^2.4.1",
        "ts-loader": "^9.3.0",
        "typescript": "^4.6.4",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2"
